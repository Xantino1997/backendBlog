{
    "build": {
      "builder": "NIXPACKS"
    },
    "deploy": {
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
    },
    "services": {
      "my-service": {
        "http": {
          "handler": "main:app",
          "cors": {
            "allowedOrigins": ["https://blog3-eta.vercel.app"],
            "allowedMethods": ["GET", "POST", "PUT"],
            "allowedHeaders": ["Content-Type", "Authorization"]
          }
        }
      }
    }
  }
  